<!DOCTYPE html>
<html>
  <head>
    <title>FileUpload</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p>
      <%= link_to "Upload new list", welcome_index_path %>
      <%= link_to "Totals", welcome_totals_path %>
      <%= link_to "Reset db", welcome_reset_path, method: :post %>
    </p>
    <%= yield %>
  </body>
</html>
